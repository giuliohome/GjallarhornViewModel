<Window
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"		
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"		
		Title="Test Functional Status Update" MinWidth="300" MinHeight="100" SizeToContent="WidthAndHeight" Height="136">
    <Grid>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="10,7,0,0" TextWrapping="Wrap" Text="{Binding Result}" VerticalAlignment="Top" Width="120"/>
        <Button Content="Change it!" Command="{Binding ChangeCommand}" HorizontalAlignment="Left" Margin="15,65,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>